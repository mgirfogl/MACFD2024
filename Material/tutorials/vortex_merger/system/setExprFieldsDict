FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setExprFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

expressions
(
    omega
    {
        field           omega;
        dimensions      [0 0 -1 0 0 0 0];

        expression
        #{
           exp(-3.14*(sqr(pos().x() - 2.355) + sqr(pos().y() - 3.14))) + exp(-3.14*(sqr(pos().x() - 3.925) + sqr(pos().y() - 3.14)))
        #};
    }
);


